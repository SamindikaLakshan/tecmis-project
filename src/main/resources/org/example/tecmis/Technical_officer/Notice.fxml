<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="423.0" prefWidth="637.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.tecmis.Technical_officer.Notice">
    <children>
        <Label alignment="TOP_CENTER" layoutX="267.0" layoutY="53.0" prefHeight="13.0" prefWidth="91.0" text="VIEW NOTICES">
            <font>
                <Font name="Agency FB Bold" size="14.0" />
            </font>
        </Label>
        <TableView fx:id="notice_table" layoutX="147.0" layoutY="112.0" prefHeight="200.0" prefWidth="359.0">
            <columns>
                <TableColumn fx:id="noticeid" prefWidth="112.0" text="Notice_id" />
                <TableColumn fx:id="topic" prefWidth="88.0" text="Topic" />
                <TableColumn fx:id="notice" prefWidth="74.0" text="Notice" />
                <TableColumn fx:id="date" prefWidth="83.0" text="Date" />
            </columns>
        </TableView>
        <Button fx:id="viewButton" layoutX="203.0" layoutY="356.0" mnemonicParsing="false" onAction="#handleView" prefHeight="32.0" prefWidth="72.0" text="VIEW">
            <font>
                <Font name="Agency FB" size="12.0" />
            </font>
        </Button>
        <Button fx:id="backButton" layoutX="351.0" layoutY="356.0" mnemonicParsing="false" onAction="#handleBack" prefHeight="32.0" prefWidth="72.0" text="BACK">
            <font>
                <Font name="Agency FB" size="12.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
